    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "@solana/web3.js": "^1.87.6",
    "joi": "^17.11.0",
    "bullmq": "^5.1.5",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
